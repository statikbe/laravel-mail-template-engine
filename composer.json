{
  "name": "statikbe/laravel-mail-template-engine",
  "description": "Customizable mail templates for Laravel",
  "license": "MIT",
  "keywords": [
    "statikbe",
    "laravel",
    "statik",
    "mail",
    "mails",
    "template",
    "custom"
  ],
  "authors": [
    {
      "name": "Kobe Christiaensen",
      "email": "kobe@statik.be"
    },
    {
      "name": "Robbe Reygel",
      "email": "robbe@statik.be"
    }
  ],
  "require": {
    "php": "^8.0|^8.1",
    "illuminate/mail": "^10.0",
    "illuminate/console": "^10.0",
    "illuminate/bus": "^10.0",
    "spatie/laravel-translatable": "^6.0"
  },
  "autoload": {
    "psr-4": {
      "Statikbe\\LaravelMailEditor\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Statikbe\\LaravelMailEditor\\MailEditorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
